import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter any number:" );
        int n=sc.nextInt();
        int sum=0;
        
        for(int i=1;i<=n;i=i+1){
            if(i%2==0){
                sum=sum+i;
            }
        }
        System.out.println("sum of all even numbers: "+sum);
    }
}
